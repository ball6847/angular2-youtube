{
  "name": "angular2-youtube",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "config": {
    "ghp": ".gh-pages-build",
    "git_build_prefix": "git -C $npm_package_ghp"
  },
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod --aot",
    "build:gh": "ng build --prod --aot --baseHref /angular2-youtube/ --outputPath .gh-pages-build",
    "deploy:gh": "npm run build:gh && npm run _init_gh && npm run _push_gh",
    "_init_gh": "GITCMD='git -C .gh-pages-build/' && $GITCMD init && $GITCMD remote add origin git@github.com:ball6847/angular2-youtube.git",
    "_push_gh": "GITCMD='git -C .gh-pages-build/' && $GITCMD checkout -b gh-pages && $GITCMD add -A && $GITCMD commit -m \"update gh-pages\" && $GITCMD push -f origin gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^1.0.0-rc.1",
    "@angular/common": "~2.4.8",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "~2.4.8",
    "@angular/forms": "~2.4.8",
    "@angular/http": "~2.4.8",
    "@angular/platform-browser": "~2.4.8",
    "@angular/platform-browser-dynamic": "~2.4.8",
    "@angular/router": "~3.4.8",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "angularfire2": "^2.0.0-beta.8",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "firebase": "^3.7.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.15.1",
    "ng2-bootstrap": "^1.3.3",
    "ng2-dragula": "^1.3.0",
    "ng2-pagination": "^2.0.1",
    "ng2-youtube-player": "github:ball6847/ng2-youtube-player",
    "ngrx-store-localstorage": "^0.1.5",
    "rxjs": "5.2.0",
    "tassign": "^1.0.0",
    "ts-helpers": "^1.1.1",
    "uuid-js": "^0.7.5",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^2.3.1",
    "@types/firebase": "^2.4.31",
    "@types/jasmine": "^2.2.30",
    "@types/lodash": "^4.14.54",
    "@types/node": "^7.0.5",
    "@types/youtube": "0.0.28",
    "codelyzer": "3.0.0-beta.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "5.1.1",
    "ts-node": "2.1.0",
    "tslint": "4.4.2",
    "typescript": "~2.2.1"
  }
}
